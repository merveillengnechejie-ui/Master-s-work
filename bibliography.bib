author    = {Naoya Aizawa and Ricardo Javier Vázquez},
  journal   = {The Journal of Physical Chemistry C},
  title     = {Thermally Activated Delayed Fluorescence (TADF) Active Systems: Mechanism, Applications, and Future Directions},
  year      = {2025},
  issn      = {1932-7447},
  number    = {7},
  pages     = {3359-3360},
  volume    = {129},
